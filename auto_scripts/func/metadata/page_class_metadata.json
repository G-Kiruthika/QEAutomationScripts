{"pages": [{"class_name": "LoginPage","file_name": "login_page","locators": {"email_field": {"strategy": "id","value": "email"},"password_field": {"strategy": "id","value": "password"},"login_button": {"strategy": "xpath","value": "//button[@type='submit']"},"error_message": {"strategy": "xpath","value": "//div[contains(@class,'error') or contains(text(),'Invalid username or password')]"}},"actions": [{"action_name": "navigate_to_login","description": "Navigates to the login page using provided URL","parameters": ["url"]},{"action_name": "enter_email","description": "Enters email address into the email field","parameters": ["email"]},{"action_name": "enter_password","description": "Enters password into the password field","parameters": ["password"]},{"action_name": "click_login","description": "Clicks the login button","parameters": []},{"action_name": "get_error_message","description": "Retrieves the error message displayed on failed login","parameters": []}],"validations": [{"validation_name": "is_login_page_displayed","description": "Validates that login page is loaded with email and password fields"},{"validation_name": "is_authenticated","description": "Validates user is authenticated and redirected to dashboard/home page"},{"validation_name": "is_session_created","description": "Validates session token generation and username display in header"},{"validation_name": "is_login_failed","description": "Validates login failure and error message presence"},{"validation_name": "is_user_on_login_page","description": "Validates that user remains on login page after failed login"}]},{"class_name": "DashboardPage","file_name": "dashboard_page","locators": {"header_username": {"strategy": "xpath","value": "//header//span[@class='username']"},"session_token": {"strategy": "js","value": "window.sessionStorage.getItem('token')"}},"actions": [{"action_name": "get_username","description": "Retrieves username from header","parameters": []},{"action_name": "get_session_token","description": "Retrieves session token from storage","parameters": []}],"validations": [{"validation_name": "is_dashboard_displayed","description": "Validates that dashboard/home page is displayed after login"}]}]}