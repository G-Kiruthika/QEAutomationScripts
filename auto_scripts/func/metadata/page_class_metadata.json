{
  "pages": {
    "RegistrationPage": {
      "file_name": "registration_page.py",
      "locators": {
        "email_input": {"by": "id", "value": "email_input_placeholder"},
        "password_input": {"by": "id", "value": "password_input_placeholder"},
        "name_input": {"by": "id", "value": "name_input_placeholder"},
        "register_button": {"by": "id", "value": "register_button_placeholder"},
        "error_message": {"by": "id", "value": "error_message_placeholder"}
      },
      "actions": [
        "register_user(email, password, name)",
        "submit_registration()"
      ],
      "validations": [
        "validate_registration_success()",
        "validate_error_message()"
      ]
    },
    "DatabasePage": {
      "file_name": "database_page.py",
      "locators": {
        "user_record": {"by": "db", "value": "user_record_placeholder"},
        "password_hash": {"by": "db", "value": "password_hash_placeholder"}
      },
      "actions": [
        "retrieve_user_record(email)"
      ],
      "validations": [
        "validate_password_hash()"
      ]
    }
  }
}