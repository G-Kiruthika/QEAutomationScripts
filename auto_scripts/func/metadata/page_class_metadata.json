{
  "pages": {
    "RegistrationPage": {
      "file_name": "registration_page.py",
      "locators": {
        "email_input": {"by": "id", "value": "email_input_placeholder"},
        "password_input": {"by": "id", "value": "password_input_placeholder"},
        "name_input": {"by": "id", "value": "name_input_placeholder"},
        "register_button": {"by": "id", "value": "register_button_placeholder"},
        "error_message": {"by": "id", "value": "error_message_placeholder"}
      },
      "actions": [
        "enter_email(email)",
        "enter_password(password)",
        "enter_name(name)",
        "submit_registration()"
      ],
      "validations": [
        "validate_registration_page_displayed()",
        "validate_fields_accept_input()",
        "validate_registration_success()",
        "validate_error_message(expected_error)"
      ]
    }
  }
}