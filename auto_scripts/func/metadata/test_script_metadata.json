{
  "tests": [
    {
      "test_name": "test_login_valid_credentials",
      "feature": "login",
      "target_file": "login_page.py",
      "pages_used": ["LoginPage", "DashboardPage"],
      "flow": [
        {"action": "navigate_to_login_page", "page": "LoginPage"},
        {"action": "enter_email", "page": "LoginPage"},
        {"action": "enter_password", "page": "LoginPage"},
        {"action": "click_login_button", "page": "LoginPage"},
        {"action": "verify_login_success", "page": "LoginPage"},
        {"action": "verify_user_session_created", "page": "LoginPage"},
        {"action": "verify_user_name_displayed", "page": "DashboardPage"}
      ],
      "test_data": {
        "url": "https://ecommerce.example.com/login",
        "email": "testuser@example.com",
        "password": "Test@1234"
      }
    },
    {
      "test_name": "test_login_invalid_username",
      "feature": "login",
      "target_file": "login_page.py",
      "pages_used": ["LoginPage"],
      "flow": [
        {"action": "navigate_to_login_page", "page": "LoginPage"},
        {"action": "enter_username", "page": "LoginPage"},
        {"action": "enter_password", "page": "LoginPage"},
        {"action": "click_login_button", "page": "LoginPage"},
        {"action": "verify_login_failure", "page": "LoginPage"},
        {"action": "verify_user_remains_on_login_page", "page": "LoginPage"}
      ],
      "test_data": {
        "url": "https://ecommerce-website.com/login",
        "username": "invaliduser@example.com",
        "password": "ValidPass123!"
      }
    }
  ],
  "framework": {
    "base_classes_required": ["BasePage"],
    "core_dependencies": ["core/driver_factory.py", "core/selenium_wrapper.py"],
    "config_dependencies": ["config/config.yaml"],
    "external_libraries": ["selenium", "pytest", "pyyaml"],
    "required_imports": ["pages.", "core.driver_factory", "pages.base_page"]
  }
}