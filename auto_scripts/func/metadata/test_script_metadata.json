{
  "test_suite": "Functional UI Suite",
  "test_cases": {
    "C38": {
      "test_name": "test_registration_with_valid_details",
      "feature": "user_registration",
      "target_file": "test_registration.py",
      "pages_used": ["RegistrationPage"],
      "flow": [
        {"page": "RegistrationPage", "action": "validate_registration_page_displayed"},
        {"page": "RegistrationPage", "action": "enter_email", "data_ref": "user_data.email"},
        {"page": "RegistrationPage", "action": "enter_password", "data_ref": "user_data.password"},
        {"page": "RegistrationPage", "action": "enter_name", "data_ref": "user_data.name"},
        {"page": "RegistrationPage", "action": "submit_registration"},
        {"page": "RegistrationPage", "assertion": "validate_registration_success"}
      ],
      "test_data": {
        "user_data": {
          "email": "user@example.com",
          "password": "SecurePass123",
          "name": "John Doe"
        }
      }
    },
    "C39": {
      "test_name": "test_registration_with_duplicate_email",
      "feature": "user_registration",
      "target_file": "test_registration.py",
      "pages_used": ["RegistrationPage"],
      "flow": [
        {"page": "RegistrationPage", "action": "enter_email", "data_ref": "user_data.email"},
        {"page": "RegistrationPage", "action": "submit_registration"},
        {"page": "RegistrationPage", "assertion": "validate_registration_success"},
        {"page": "RegistrationPage", "action": "enter_email", "data_ref": "user_data.email"},
        {"page": "RegistrationPage", "action": "submit_registration"},
        {"page": "RegistrationPage", "assertion": "validate_error_message", "expected_error": "Email already exists."}
      ],
      "test_data": {
        "user_data": {
          "email": "user@example.com"
        }
      }
    }
  },
  "framework": {
    "architecture": "python-ui-automation-framework",
    "type": "ui",
    "base_classes_required": ["BasePage"],
    "core_dependencies": ["core/driver_factory.py", "core/selenium_wrapper.py"],
    "api_dependencies": [],
    "config_dependencies": ["config/config.yaml"],
    "utils_dependencies": [],
    "external_libraries": ["selenium", "pytest", "pyyaml"],
    "required_imports": ["pages.", "core.driver_factory", "pages.base_page"]
  }
}