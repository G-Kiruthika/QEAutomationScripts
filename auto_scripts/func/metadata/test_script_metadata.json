{
  "tests": [
    {
      "test_name": "test_login_valid_credentials",
      "feature": "User Authentication",
      "target_file": "test_login_valid_credentials.py",
      "pages_used": ["LoginPage", "DashboardPage"],
      "flow": [
        {"action": "navigate_to_login_page", "page": "LoginPage", "test_data": {"url": "https://ecommerce.example.com/login"}},
        {"assertion": "is_login_page_displayed", "page": "LoginPage"},
        {"action": "enter_email", "page": "LoginPage", "test_data": {"email": "testuser@example.com"}},
        {"assertion": "is_email_accepted", "page": "LoginPage"},
        {"action": "enter_password", "page": "LoginPage", "test_data": {"password": "Test@1234"}},
        {"assertion": "is_password_accepted", "page": "LoginPage"},
        {"action": "click_login_button", "page": "LoginPage"},
        {"assertion": "is_user_authenticated", "page": "LoginPage"},
        {"assertion": "is_dashboard_displayed", "page": "DashboardPage"},
        {"action": "verify_user_session", "page": "DashboardPage"},
        {"assertion": "is_user_header_displayed", "page": "DashboardPage"},
        {"assertion": "is_session_token_generated", "page": "LoginPage"}
      ],
      "test_data": {
        "url": "https://ecommerce.example.com/login",
        "email": "testuser@example.com",
        "password": "Test@1234"
      },
      "framework": {
        "base_classes_required": ["BasePage", "BaseTest"],
        "core_dependencies": ["selenium", "pytest"],
        "config_dependencies": ["config.yaml", "testdata.yaml"],
        "utils_dependencies": ["error_handler.py", "reporter.py"],
        "external_libraries": ["pytest", "selenium"],
        "required_imports": ["from pages.login_page import LoginPage", "from pages.dashboard_page import DashboardPage", "from utils.error_handler import ErrorHandler", "from utils.reporter import Reporter"]
      }
    },
    {
      "test_name": "test_login_invalid_username",
      "feature": "User Authentication",
      "target_file": "test_login_invalid_username.py",
      "pages_used": ["LoginPage"],
      "flow": [
        {"action": "navigate_to_login_page", "page": "LoginPage", "test_data": {"url": "https://ecommerce-website.com/login"}},
        {"assertion": "is_login_page_displayed", "page": "LoginPage"},
        {"action": "enter_email", "page": "LoginPage", "test_data": {"email": "invaliduser@example.com"}},
        {"assertion": "is_email_accepted", "page": "LoginPage"},
        {"action": "enter_password", "page": "LoginPage", "test_data": {"password": "ValidPass123!"}},
        {"assertion": "is_password_accepted", "page": "LoginPage"},
        {"action": "click_login_button", "page": "LoginPage"},
        {"assertion": "is_error_message_displayed", "page": "LoginPage", "test_data": {"error_message": "Invalid username or password"}},
        {"assertion": "is_still_on_login_page", "page": "LoginPage"}
      ],
      "test_data": {
        "url": "https://ecommerce-website.com/login",
        "email": "invaliduser@example.com",
        "password": "ValidPass123!",
        "error_message": "Invalid username or password"
      },
      "framework": {
        "base_classes_required": ["BasePage", "BaseTest"],
        "core_dependencies": ["selenium", "pytest"],
        "config_dependencies": ["config.yaml", "testdata.yaml"],
        "utils_dependencies": ["error_handler.py", "reporter.py"],
        "external_libraries": ["pytest", "selenium"],
        "required_imports": ["from pages.login_page import LoginPage", "from utils.error_handler import ErrorHandler", "from utils.reporter import Reporter"]
      }
    }
  ]
}
