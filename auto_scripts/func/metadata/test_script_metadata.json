      "test_name": "test_login_username_blank",
      "feature": "Login",
      "target_file": "test_login_username_blank.py",
      "pages_used": ["LoginPage"],
      "flow": [
        {"page": "LoginPage", "action": "navigate_to_login_page", "data_ref": {"url": "https://ecommerce-website.com/login"}},
        {"page": "LoginPage", "action": "enter_username", "data_ref": {"username": ""}},
        {"page": "LoginPage", "action": "enter_password", "data_ref": {"password": "ValidPass123!"}},
        {"page": "LoginPage", "action": "click_login_button", "data_ref": null},
        {"page": "LoginPage", "assertion": "is_validation_error_displayed"},
        {"page": "LoginPage", "assertion": "is_login_prevented"}
      ],
      "test_data": {
        "url": "https://ecommerce-website.com/login",
        "username": "",
        "password": "ValidPass123!"
      }
    },
    "510": {
      "test_name": "test_login_account_lockout",
      "feature": "Login",
      "target_file": "test_login_account_lockout.py",
      "pages_used": ["LoginPage"],
      "flow": [
        {"page": "LoginPage", "action": "navigate_to_login_page", "data_ref": {"url": "https://ecommerce-website.com/login"}},
        {"page": "LoginPage", "action": "enter_username", "data_ref": {"username": "validuser@example.com"}},
        {"page": "LoginPage", "action": "enter_password", "data_ref": {"password": "WrongPass1"}},
        {"page": "LoginPage", "action": "click_login_button", "data_ref": {"attempts": 5, "passwords": ["WrongPass1", "WrongPass2", "WrongPass3", "WrongPass4", "WrongPass5"]}},
        {"page": "LoginPage", "assertion": "is_validation_error_displayed"},
        {"page": "LoginPage", "action": "enter_password", "data_ref": {"password": "ValidPass123!"}},
        {"page": "LoginPage", "action": "click_login_button", "data_ref": null},
        {"page": "LoginPage", "assertion": "is_account_locked"},
        {"page": "LoginPage", "assertion": "is_account_lockout_notification_sent"}
      ],
      "test_data": {
        "url": "https://ecommerce-website.com/login",
        "username": "validuser@example.com",
        "passwords": ["WrongPass1", "WrongPass2", "WrongPass3", "WrongPass4", "WrongPass5", "ValidPass123!"]
      }
    }
